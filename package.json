{
  "name": "hospital-finance-dashboard",
  "version": "1.0.0",
  "description": "A comprehensive hospital finance dashboard built with React and TypeScript",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov --coverageReporters=html",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:all": "npm run test:ci && npm run test:integration && npm run test:e2e",
    "analyze": "vite build && open dist/stats.html",
    "analyze:ci": "vite build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:scan": "snyk test",
    "docs:build": "typedoc src --out docs/api --theme default",
    "docs:serve": "serve docs -p 8080",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@sentry/browser": "^10.12.0",
    "@sentry/react": "^10.12.0",
    "@sentry/tracing": "^7.120.4",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.284.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.0",
    "@playwright/test": "^1.40.0",
    "@sentry/types": "^10.12.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.14",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-testing-library": "^6.1.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "serve": "^14.2.1",
    "snyk": "^1.1248.0",
    "tailwindcss": "^3.3.5",
    "terser": "^5.22.0",
    "ts-jest": "^29.4.2",
    "ts-jest-mock-import-meta": "^1.3.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.5",
    "zod": "^3.22.4"
  },
  "keywords": [
    "hospital",
    "finance",
    "dashboard",
    "react",
    "typescript"
  ],
  "author": "Hospital Finance Team",
  "license": "MIT"
}
